{
  "name": "Youtube Comment Layout",
  "version": "1",
  "manifest_version": 2,
  "description": "Extension for custom youtube comment layout",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon/icon16.png",
    "32": "icon/icon32.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "permissions": ["activeTab"]
}
